<header>
<h1>TikTok</h1>
<p>Welcome, <a href="/profile/<%=session[:user]["id"]%>"><%=session[:user]["name"]%></a>| <a href="../logout">logout</a>
</header>
<h2><%=session[:user]["name"]%>'s Follows</h2>

<table>                         
  <tr>                          
    <th>Username</th>
    <th>Date</th>
    <th>Action</th>
  </tr>
<% @data.each do |data| %>
  <tr>
    <td><%= data.name %></td>
    <td><%= data.created_at.strftime("%B %d, %Y") %></td>
    <td><a href="/unfollow/<%=data.id%>">Unfollow</a></td>
  </tr>
<% end %>
</table>    